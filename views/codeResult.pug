extends layout
block content
  - if (results.link)
    h1 You can download your SDK from the following url
    h2
      a(href=results.link.replace('localhost:8080','api.openapi-generator.tech')) Courtesy of OpenAPI-Generator.tech
  - else
    h1 Something went wrong...
    h2='Code '+results.code
    h4=results.message
  br
  br
  form.return(method='get', action='/'+apiName)
    input(type='submit', value='Return to API', class='button')
